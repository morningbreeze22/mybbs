<p id="notice"><%= notice %></p>
<br>
<h1>Lists</h1>
<br>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Userid</th>
      <th>Phone</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= list.userid %></td>
        <td><%= list.phone %></td>
        <td><%= list.role %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '创建新用户', new_list_path %>
